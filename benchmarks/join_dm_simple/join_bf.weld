# SELECT SUM(a+b+c+d+z) 
# FROM R,S
# WHERE R.y = S.y
|R_x:vec[i64],R_y:vec[i64],S_y:vec[i64],S_z:vec[i64]|
let S_blds = for(
    zip(S_y, S_z),
    {dictmerger[i64,i64,+], bloombuilder[i64](len(S_y))},
    |bs,i,e| {merge(bs.$0, e), merge(bs.$1, e.$0)}
);
let S_ht = result(S_blds.$0);
let S_ht_bf = result(S_blds.$1);

result(for(
    zip(R_x,R_y),
    merger[i64,+],
    |b,i,e|
        let x = e.$0;
        let y = e.$1;
        if (bfcontains(S_ht_bf, y),
            if (keyexists(S_ht, y),
                let z = lookup(S_ht, y);
                merge(b, x+y+z),
                b
            ),
            b
        )
        
))